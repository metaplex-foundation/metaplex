{
  "name": "@metaplex/graphql",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/metaplex/graphql"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint:format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint": "yarn lint:format && yarn lint:eslint --fix",
    "build": "yarn clean && tsc",
    "start": "node dist/index.js",
    "server": "ts-node-dev --transpile-only src/index.ts",
    "dev": "yarn server",
    "tsc": "tsc",
    "tscCheck": "tsc --noEmit"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/dedent": "^0.7.0",
    "@types/graphql": "^14.5.0",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.2",
    "husky": "^4.3.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --cache --fix --max-warnings=0"
    ],
    "*.{js,json}": "prettier --write"
  },
  "dependencies": {
    "@discoveryjs/json-ext": "^0.5.5",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.21.0",
    "apollo-datasource": "^3.1.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "bn.js": "^5.1.3",
    "bs58": "^4.0.1",
    "cross-fetch": "^3.1.4",
    "dedent": "^0.7.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "graphql-subscriptions": "^1.2.1",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "nexus": "^1.1.0",
    "queue": "^6.0.2",
    "subscriptions-transport-ws": "^0.9.19"
  }
